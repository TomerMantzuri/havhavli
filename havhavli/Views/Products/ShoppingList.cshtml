@model havhavli.Models.Product

@{
    ViewData["Title"] = "סל קניות";
}

<h1>סל קניות של ליעם</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
